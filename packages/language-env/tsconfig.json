{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "preserve",
    /* Type Checking */
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "exactOptionalPropertyTypes": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true
  },
  "include": ["./**/*.ts"]
}
